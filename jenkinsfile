pipeline{
    agent any

    stages{
        stage('Code'){
            steps{
                git url: 'git@github.com:yogendra-kokamkar/Terraform_ECS_Express.git', branch: 'main'
            }
        }

        stage('Connect to docker server'){
            steps{
            sshagent(credentials: ['DockerSshEc2']) {
                sh "pwd"
             }
            }
        }
    }
}